<script type="application/ld+json">
    {
        "@context":"https://schema.org",
        "@type": "MusicGroup",
        "name": "Tunkal",
        "description": "{{ post.metaDescription ?? '' }}",
    {% if banner is defined and banner != false %}
        "logo": {
            "@type": "ImageObject",
            "url": "{{ url('home') ~ 'images/' ~ banner.image }}"
        },
    {% endif %}
    {% if post.images|length > 0 %}
        "image": {
            "@type": "ImageObject",
            "url": "{{ url('home') ~ 'images/' ~ post.images[0].name }}"
        },
    {% endif %}
        "url": "{{ url('home') }}",
        "genre": [
            "Metal",
            "Death Metal",
            "Thrash Metal"
        ],
        "sameAs": [
        {% for link in links()|filter( link => link.position == 'header' ) %}
            "{{ link.url }}"{{ loop.last ? '' : ',' }}
        {% endfor %}
        ],
        "foundingLocation": {
            "@type": "Country",
            "name": "France"
        },
        "foundingDate": "2018",
        "track": [
        {% for playlistItem in playlistItems %}
        {
            "@type": "VideoObject",
            "name": "{{ playlistItem.snippet.title }}",
            "description": "{{ playlistItem.snippet.title }}",
            "embedUrl": "https://www.youtube.com/watch?v={{ playlistItem.contentDetails.videoId }}",
            "thumbnailUrl":"{{ playlistItem.snippet.thumbnails.medium.url }}",
            "uploadDate":"{{ playlistItem.snippet.publishedAt }}"
        }{{ loop.last ? '' : ',' }}
        {% endfor %}
        ],
        "member": [
            {
            "@type": "OrganizationRole",
            "member": {
                "@type": "Person",
                "name": "Tunkal"
            },
            "startDate": "2018",
            "roleName": "guitar"
            }
        ]
    }
</script>


